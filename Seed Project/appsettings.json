{
  "Serilog": {
    "Using": [ "Serilog.Sinks.MSSqlServer" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "MSSqlServer",
        "Args": {
          "connectionString": "Data Source=.; Initial Catalog=jobs;Integrated Security=true", // connection String
          "tableName": "logs" // table name
        }
      }
    ]
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "IdentityContextConnection": "Server=.\\SQL2016;Database=SeedProject;Trusted_Connection=True;MultipleActiveResultSets=true"
  }
}