{
  "Serilog": {
    "Using": [ "Serilog.Sinks.MSSqlServer" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "MSSqlServer",
        "Args": {
          "connectionString": "Data Source=.\\SQL2016; Initial Catalog=SeedProject1;Integrated Security=true", // connection String
          "tableName": "logs" // table name
        }
      }
    ]
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "IdentityContextConnection": "Data Source=.\\SQL2016;Initial Catalog=SeedProject1;Integrated Security=true;MultipleActiveResultSets=True;"
  }
}